module.exports = {
  command: "kick",
  run: async (sock, msg, target) => {
    try {
      await sock.groupParticipantsUpdate(msg.key.remoteJid, [target], "remove")
      await sock.sendMessage(msg.key.remoteJid, { text: `✅ L'utilisateur a été exclu !` })
    } catch (err) {
      await sock.sendMessage(msg.key.remoteJid, { text: `❌ Impossible de kicker : ${err.message}` })
    }
  }
}

module.exports = {
  command: "tagall",
  run: async (sock, msg, participants) => {
    if (!participants || participants.length === 0) return
    let text = "ğŸ‘¥ Tagging everyone:\n"
    for (let p of participants) {
      text += `@${p.split("@")[0]} `
    }
    await sock.sendMessage(msg.key.remoteJid, { 
      text,
      mentions: participants
    })
  }
}
